{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "current"
        }
      }
    ],
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ],
    "@babel/preset-typescript"
  ],
  "plugins": ["babel-plugin-react-compiler"],
  "env": {
    "test": {
      "plugins": ["babel-plugin-transform-vite-meta-env"]
    }
  }
}

{
  "name": "lorebrary-web",
  "version": "1.0.0",
  "description": "A template for React projects using Parcel",
  "source": "src/index.html",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "start": "parcel --open -p 3000",
    "clean": "rmdir /s dist",
    "build": "parcel build --public-url ./",
    "test": "jest --passWithNoTests",
    "lint": "eslint .",
    "pretty": "prettier --write .",
    "pretty-check": "prettier --check .",
    "prepare": "npx simple-git-hooks"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Two-Boys-and-a-Dream/lorebrary-web.git"
  },
  "keywords": [
    "React",
    "Template",
    "Boilerplate"
  ],
  "author": "Raymond-Cox",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Two-Boys-and-a-Dream/lorebrary-web/issues"
  },
  "homepage": "https://github.com/Two-Boys-and-a-Dream/lorebrary-web#readme",
  "dependencies": {
    "@chakra-ui/icons": "^2.2.4",
    "@chakra-ui/react": "^2.7.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@tanstack/react-query": "^5.90.9",
    "@tanstack/react-query-devtools": "^5.90.2",
    "axios": "^1.13.2",
    "framer-motion": "^12.23.24",
    "luxon": "^3.7.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@parcel/babel-plugin-transform-runtime": "^2.16.1",
    "@parcel/babel-preset-env": "^2.16.1",
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/luxon": "^3.7.1",
    "babel-jest": "^30.2.0",
    "buffer": "^6.0.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-transform-stub": "^2.0.0",
    "parcel": "^2.16.1",
    "prettier": "^3.6.2",
    "process": "^0.11.10",
    "simple-git-hooks": "^2.13.1"
  },
  "simple-git-hooks": {
    "pre-commit": "npm run pretty",
    "pre-push": "npm run lint"
  }
}
